prefix=/app
libdir=${prefix}/lib
includedir=${prefix}/include

Name: GExiv2
Description: GObject bindings for exiv2
URL: https://wiki.gnome.org/Projects/gexiv2
Version: 0.14.0
Requires: glib-2.0, gobject-2.0 >=  2.46.0, gio-2.0 >=  2.46.0
Requires.private: exiv2 >=  0.26
Libs: -L${libdir} -lgexiv2
Cflags: -I${includedir}
